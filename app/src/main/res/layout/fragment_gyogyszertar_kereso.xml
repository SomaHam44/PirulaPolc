<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GyogyszertarKeresoFragment">

    <TextView
        android:id="@+id/gyogyszertarKeres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="106dp"
        android:text="Gyógyszertár kereső"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/gyogyszertarTerkep"
        android:layout_width="360dp"
        android:layout_height="400dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/gyogyszertarKeres"
        app:layout_constraintTop_toBottomOf="@+id/gyogyszertarKeres" />

    <Button
        android:id="@+id/btnGyogyszertarKeresVissza"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Vissza"
        app:layout_constraintStart_toStartOf="@+id/gyogyszertarTerkep"
        app:layout_constraintTop_toBottomOf="@+id/gyogyszertarTerkep" />

</androidx.constraintlayout.widget.ConstraintLayout>